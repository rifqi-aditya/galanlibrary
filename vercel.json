{
    "version": 2,
      "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.4" }
    },
    "routes": [
        {
            "src": "/(css|js|img|images|assets|fonts|storage)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/([^/]+\\.(?:jpg|jpeg|png|gif|svg|ico))$",
            "dest": "public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}